 # General Aliases
    alias apk='apk --progress'
    alias ls="ls -ltra"
    alias status="pidof keepalived | kill -s USR1; cat /tmp/keepalived.data"

    alias hosts='cat /etc/hosts'
    alias ..="cd .."
    alias ...="cd ../.."
    alias ....="cd ../../.."
    alias untar="tar xzvkf"
    alias mv="mv -nv"
    alias cp="cp -i"
    alias ip4="ip -4 addr"
    alias ip6="ip -6 addr"
